<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="EmptyView">
        <attr name="image" format="reference" />
        <attr name="imageHeight" format="dimension" />
        <attr name="description" format="string" />
        <attr name="btName" format="string" />
    </declare-styleable>

    <declare-styleable name="AppBar">
        <attr name="bgColor" format="color" />
        <attr name="titleStr" format="string" />
        <attr name="titleIcon" format="reference" />
        <attr name="useTabs" format="boolean" />
    </declare-styleable>

    <declare-styleable name="HeaderCommon">
        <attr name="title" format="string|reference" />
        <attr name="leftBtn" format="enum|reference">
            <enum name="none" value="0" />
            <enum name="back" value="1" />
            <enum name="next" value="2" />
            <enum name="close" value="3" />
            <enum name="info" value="4" />
        </attr>
        <attr name="rightBtn" format="enum|reference">
            <enum name="none" value="0" />
            <enum name="back" value="1" />
            <enum name="next" value="2" />
            <enum name="close" value="3" />
            <enum name="info" value="4" />
        </attr>
    </declare-styleable>

</resources>